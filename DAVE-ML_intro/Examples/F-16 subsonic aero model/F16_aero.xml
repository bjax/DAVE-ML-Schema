<?xml version="1.0" standalone="no"?>
<!DOCTYPE DAVEfunc SYSTEM "DAVEfunc.dtd">
<DAVEfunc>
    <fileHeader>

        <author name="Bruce Jackson" org="NASA Langley Research Center" xns="@bjax"/>
        <fileCreationDate date="28-MAR-2002"/>
        <description>
	  F-16 Aero Data file. Based on Morelli's adaptation of
          Stevens and Lewis' F-16 example [1] described in Garza &amp;
          Morelli's TM [2]. Obtained from E. A. Morelli in the form of
          Matlab scripts [3] &amp; [4]
	</description>

      <!-- ================== -->
      <!--     References     -->
      <!-- ================== -->

      <reference refID="REF01" author="Stevens, Brian L. and Lewis, Frank L."
	      title="Aircraft Control and Simulation"
	      accession="ISBN O-471-61397-5" date="1992"/>

      <reference refID="REF02" author="Garza, F. R.; and Morelli, E. A."  
		title="A Collection of Nonlinear Aircraft Simulations
		in MATLAB" accession="NASA TM-2002-xxxxxx" date="JUN-2002"/>

      <reference refID="REF03" author="Morelli, Eugene A."
	      title="f16_aero.m" date="17-JUN-1995"/>

      <reference refID="REF04" author="Morelli, Eugene A."
	      title="f16_aero_setup.m" date="17-JUN-1995"/>

    </fileHeader>

<!-- from reference 3
function [cx,cy,cz,cl,cm,cn] = f16_aero(vt,alpha,beta,p,q,r,el,ail,rdr,xcg)
%
%  function [cx,cy,cz,cl,cm,cn] = f16_aero(vt,alpha,beta,p,q,r,el,ail,rdr,xcg)
%
%  Usage: [cx,cy,cz,cl,cm,cn] = f16_aero(vt,alpha,beta,p,q,r,el,ail,rdr,xcg);
%
%  Description:
%
%    Computes non-dimensional aerodynamic force and moment 
%    coefficients for the F-16 nonlinear simulation.
%
%
%  Input:
%    
%     vt  = airspeed, ft/sec.
%   alpha = angle of attack, deg.
%    beta = sideslip angle, deg.
%     p   = roll rate, rad/sec.
%     q   = pitch rate, rad/sec.
%     r   = yaw rate, rad/sec.
%    el   = elevator deflection, deg.
%    ail  = aileron deflection, deg.
%    rdr  = rudder deflection, deg.
%    xcg  = longitudinal c.g. location in fraction of the m.a.c.
%
%  output:
%
%    cx = body axis x force coefficient.
%    cy = body axis y force coefficient.
%    cz = body axis z force coefficient.
%    cl = rolling moment coefficient.
%    cm = pitching moment coefficient.
%    cn = yawing moment coefficient.
%

%
%    Calls:
%      cxo.m, czo.m
%      cmo.m, clo.m, cno.m
%      dlda.m, dldr.m
%      dnda.m, dndr.m
%      dampder.m
%      
%    Author [of f16_aero.m]:  Eugene A. Morelli
%
%    History [of f16_aero.m]:  
%     05 June 1995 - Created and debugged, EAM.
%
%
%  [f16_aero.m] Copyright (C) 2000  Eugene A. Morelli
%
%
%  This program [f16_aero.m] carries no warranty, not even the implied 
%  warranty of merchantability or fitness for a particular purpose.  
%
%  Please email bug reports or suggestions for improvements [to f16_aero.m] to:
%
%      e.a.morelli@larc.nasa.gov
%
global CXO CZO CMO CLO CNO DDER
global DLDA DLDR DNDA DNDR
rtd=180/pi;
xcgr=0.35;
cbar=11.32;bspan=30.;
del=el/25.0;dail=ail/20.0;drdr=rdr/30.0;
%
%  Aerodynamic forces.
%
cxt=cxo(alpha,el);
cy=-0.02*beta+0.021*dail+0.086*drdr;	% EBJ: renamed to cy0 for XML
czt=czo(alpha);
cz=czt*(1.-(beta/rtd)^2)-0.19*del;	% EBJ: this cz renamed cz1 for XML uniqueness
%
%  Aerodynamic moments.
%
clt=clo(alpha,beta);
dclda=dlda(alpha,beta);
dcldr=dldr(alpha,beta);
cl=clt + dclda*dail + dcldr*drdr;	% EBJ: this cl renamed cl1 for XML uniqueness
cmt=cmo(alpha,el);
cnt=cno(alpha,beta);
dcnda=dnda(alpha,beta);
dcndr=dndr(alpha,beta);
cn=cnt + dcnda*dail + dcndr*drdr;	% EBJ: this cn renamed cn1 for XML uniqueness
%
%  Add damping derivative contributions 
%  and cg position terms.  
%
tvt=2*vt;b2v=bspan/tvt;cq2v=cbar*q/tvt;
d=dampder(alpha);
cx=cxt + cq2v*d(1);			% EBJ: d(1) is cxq
cy=cy + b2v*(d(3)*p + d(2)*r);		% EBJ: d(2) is cyr, d(3) is cyp
cz=cz + cq2v*d(4);			% EBJ: d(4) is czq
cl=cl + b2v*(d(6)*p + d(5)*r);		% EBJ: d(5) is clr, d(6) is clp
cm=cmt + cq2v*d(7) + cz*(xcgr-xcg);	% EBJ: d(7) is cmq
cn=cn + b2v*(d(9)*p + d(8)*r)...	% EBJ: d(8) is cnr, d(9) is cnp
   - cy*(xcgr-xcg)*cbar/bspan;
return
-->
      <!-- ================== -->
      <!--  Input variables   -->
      <!-- ================== -->

  <variableDef name ="True_Airspeed_f_p_s" varID="vt" units= "ft/sec">
    <description>
	True airspeed, ft/sec
    </description>
  </variableDef>

  <variableDef name="Angle_of_Attack_deg" varID="alpha" units="deg" symbol="#x3B1">
    <description>
       Instantaneous true angle-of-attack, in degrees
    </description>
  </variableDef>
  
  <variableDef name="Angle_of_Sideslip_deg" varID="beta" units="deg" symbol="#x3B2" sign="+wind in right ear">
    <description>
       Instantaneous true angle-of-sideslip, in degrees
    </description>
  </variableDef>

  <variableDef name="s_Body_Roll_Rate_rad_p_s" varID="p" units="rad/sec" sign="+right wing down">
    <description>
	Body roll rate, rad/sec
    </description>
  </variableDef>

  <variableDef name=s_Body_Pitch_Rate_rad_p_s" varID="q" units="rad/sec" sign="+aircraft nose up">
    <description>
	Body pitch rate, rad/sec
    </description>
  </variableDef>

  <variableDef name="s_Body_Yaw_Rate_rad_p_s" varID="r" units="rad/sec" sign="+aircraft nose right">
    <description>
	Body yaw rate, rad/sec
    </description>
  </variableDef>

  <variableDef name="delta elevator" varID="el" units="deg" sign="+trailing edge down" symbol="#x3B4e">
    <description>
      Elevator deflection angle, in degrees (+TED)
    </description>
  </variableDef>

  <variableDef name="delta aileron" varID="ail" units="deg" sign="+left roll" symbol="#x3B4a">
    <description>
      Aileron deflection angle, in degrees (+LWD)
    </description>
  </variableDef>

  <variableDef name="delta rudder" varID="rdr" units="deg" sign="+left yaw" symbol="#x3B4e">
    <description>
      Rudder deflection angle, in degrees (+TEL)
    </description>
  </variableDef>

  <variableDef name="Xcg" varID="xcg" units="fract" sign="+aft">
    <description>
	CG location relative to wing leading edge, expressed as a
	fraction of aerodynamic chord length
    </description>
  </variableDef>

      <!-- ================== -->
      <!--      Constants     -->
      <!-- ================== -->

  <variableDef name="rtd" varID="rtd" units="deg/rad">
    <description>
	Conversion constant from radians to degrees
    </description>
    <calculation>
      <math>
	<apply>
	<quotient/>
	  <cn>180.</cn>
	  <cn>3.14159265</cn>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="xcgr" varID="xcgr" units="fract" initialValue="0.35">
    <description>
	Default location of center of gravity relative to wing leading
	edge, expressed as a fraction of aerodynamic chord length.
    </description>
  </variableDef>

  <variableDef name="cbar" varID="cbar" units="ft" initialValue="11.32">
    <description>
	Length of aerodynamic chord, ft
    </description>
  </variableDef>

  <variableDef name="bspan" varID="bspan" units="ft" initialValue="30.">
    <description>
	Length of aerodynamic span, ft
    </description>
  </variableDef>

      <!-- ================== -->
      <!--  Local variables   -->
      <!-- ================== -->

  <variableDef name="del" varID="del" units="">
    <description>
	Normalized elevator deflection (note range is beyond breakpoint range)
    </description>
    <calculation>
      <math>
	<apply>
	  <quotient/>
	  <ci>el</ci>
	  <cn>25.0</cn>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="dail" varID="dail" units="">
    <description>
	Normalized aileron deflection.
    </description>
    <calculation>
      <math>
	<apply>
	  <quotient/>
	  <ci>ail</ci>
	  <cn>20.0</cn>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="drdr" varID="drdr" units="">
    <description>
	Normalized rudder deflection.
    </description>
    <calculation>
      <math>
	<apply>
	  <quotient/>
	  <ci>rdr</ci>
	  <cn>30.0</cn>
	</apply>
      </math>
    </calculation>
  </variableDef>

<!-- Aerodynamic Forces -->

  <variableDef name="CX0" varID="cxt" units="" sign="+forward">
    <description>
      Basic coefficient of force in the X-body direction
      (+FWD). Function table based on angle of attack and elevator
      deflection.
    </description>
  </variableDef>

  <variableDef name="CY0" varID="cy0" units="" sign="+right">
    <description>
	Basic coefficient of force in the Y-body direction
	(+right). Function of sideslip angle, aileron and rudder
	deflection.
    </description>
    <calculation>
      <math>
	<apply>
	  <plus/>
	  <apply>
	    <times/>
	    <cn>-0.02</cn>
	    <ci>beta</ci>
	  </apply>
	  <apply>
	    <times/>
	    <cn>0.021</cn>
	    <ci>dail</ci>
	  </apply>
	  <apply>
	    <times/>
	    <cn>0.086</cn>
	    <ci>drdr</ci>
	  </apply>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="CZ0" varID="czt" units="" sign="+down">
    <description>
      Basic coefficient of force in the Z-body direction
      (+DWN). Function table based on angle of attack and elevator
      deflection.
    </description>
  </variableDef>

  <variableDef name="CZ1" varID="cz1" units="" sign="+down">
    <description>
      Coefficient of force in the Z-body direction, modified from
      basic CZ0 to include effects of sideslip and elevator
      deflection.
    </description>
    <calculation>
      <math>	<!-- cz=czt*(1.-(beta/rtd)^2)-0.19*del;	% I've renamed to cz1 for XML	-->
	<apply>				<!-- [czt*(1.-(beta/rtd)^2)] + [-0.19*del]	-->
	  <plus/>
	  <apply>			<!--  [czt] * [(1.-(beta/rtd)^2)]		-->
	    <times/>
	    <ci>czt</ci>
	    <apply>			<!--          ([1.] + [-(beta/rtd)^2)]		-->
	      <plus/>
	      <cn>1.</cn>
	      <apply>			<!--                   -[(beta/rtd)^2]		-->
		<minus/>
		<apply>			<!--                   [beta/rtd]^2		-->
		  <power/>
		  <apply>		<!--                   [beta]/[rtd]		-->
		    <quotient/>
		    <ci>beta</ci>
		    <ci>rtd</ci>
		  </apply>		<!--                   [beta]/[rtd]		-->
		  <cn>2</cn>
		</apply>		<!--                   [beta/rtd]^2		-->
	      </apply>			<!--                   -[(beta/rtd)^2]		-->
	    </apply>			<!--          ([1.] + [-(beta/rtd)^2)]		-->
	  </apply>			<!--  [czt] * [(1.-(beta/rtd)^2)]		-->
	  <apply>			<!--                          [-0.19]*[del]	-->
	    <times/>
	    <cn>-0.19</cn>
	    <ci>del</ci>
	  </apply>			<!--                          [-0.19]*[del]	-->
	</apply>			<!-- [czt*(1.-(beta/rtd)^2)] + [-0.19*del]	-->
      </math>
    </calculation>
  </variableDef>

  <!-- Aerodynamic moments -->

  <variableDef name="Cl1" varID="cl1" units="">
    <description>
      Initial value of rolling moment, less damping terms
    </description>
    <calculation>   <!-- cl=clt + dclda*dail + dcldr*drdr;	% EBJ: this cl renamed to cl1 for XML uniqueness -->
      <math>
	<apply>
	  <plus/>
	  <ci>clt</ci>
	  <apply>
	    <times/>
	    <ci>dclda</ci>
	    <ci>dail</ci>
	  </apply>
	  <apply>
	    <times/>
	    <ci>dcldr</ci>
	    <ci>drdr</ci>
	  </apply>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="Cn" varID="cn1" units="">
    <description>
	Initial value of rolling moment coefficient without damping terms
    </description>
    <calculation>	<!-- cn=cnt + dcnda*dail + dcndr*drdr;	% EBJ: this cn renamed cn1 for XML uniqueness -->
      <math>
	<apply>
	  <plus/>
	  <ci>cnt</ci>
	  <apply>
	    <times/>
	    <ci>dcnda</ci>
	    <ci>dail</ci>
	  </apply>
	  <apply>
	    <times/>
	    <ci>dcndr</ci>
	    <ci>drdr</ci>
	  </apply>
	</apply>
      </math>
    </calculation>
  </variableDef>


  <!-- Add damping derivatives and cg position terms -->

  <variableDef name="tvt" varID="tvt" units="ft/sec">
    <description>Twice velocity.</description>
    <calculation>
      <math>
	<apply>
	  <times/>
	  <cn>2</cn>
	  <ci>vt</ci>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="b2v" varID="b2v" units="">
    <description>
	Wing span over 2 times velocity. Used for coefficient normalization.
    </description>
    <calculation>
      <math>
	<apply>
	  <quotient/>
	  <ci>bspan</ci>
	  <ci>tvt</ci>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="cq2v" varID="cq2v" units="">
    <description>
	Chord times pitch rate over 2 times velocity. Used to normalize aero coefficients.
    </description>
    <calculation>
      <math>
	<apply>
	  <times/>
	  <ci>cbar</ci>
	  <apply>
	    <quotient/>
	    <ci>q</ci>
	    <ci>tvt</ci>
	  </apply>
	</apply>
      </math>
    </calculation>
  </variableDef>

	
  <!-- Function table lookup support variables -->

  <variableDef name="absbeta" varID="absbeta" units="deg">
    <description>
       Absolute value of angle-of-sideslip, deg.
    </description>
    <calculation>
      <math>
        <apply><abs/><ci>beta</ci></apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="absCl0" varID="absCl0" units="">
    <description>
       Absolute value of rolling moment coefficient
    </description>
  </variableDef>
  
  <variableDef name="absCn0" varID="absCn0" units="">
    <description>
       Absolute value of yawing moment coefficient
    </description>
  </variableDef>

  <variableDef name="Cl0" varID="clt" units="" sign="+right wing down">
    <description>
      Basic coefficient of moment around the X-body direction (roll) (+RWD)
    </description>
    <calculation>
      <math>
        <apply>
	  <piecewise>
	    <piece>
	      <apply><minus/><ci>absCl0</ci></apply>
	      <apply><lt/><ci>beta</ci><cn>0</cn></apply>
	    </piece>
	    <otherwise>
	      <ci>absCl0</ci>
	    </otherwise>
	  </piecewise>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="Cm0" varID="cmt" units="" sign="+nose up">
    <description>
      Basic coefficient of moment around the Y-body direction (pitch) (+ANU)
    </description>
  </variableDef>

  <variableDef name="Cn0" varID="cnt" units="" sign="+nose right">
    <description>
      Basic coefficient of moment around the Z-body direction (yaw) (+RWD)
    </description>
    <calculation>
      <math>
        <apply>
	  <piecewise>
	    <piece>
	      <apply><minus/><ci>absCn0</ci></apply>
	      <apply><lt/><ci>beta</ci><cn>0</cn></apply>
	    </piece>
	    <otherwise>
	      <ci>absCn0</ci>
	    </otherwise>
	  </piecewise>
        </apply>
      </math>
    </calculation>
  </variableDef>

<!-- Damping derivative tables -->

  <variableDef name="CXq" varID="cxq" units="per rad" sign="+fwd/+up">
    <description>
      Damping derivative: axial force increment per radian/sec of pitch rate
    </description>
  </variableDef>

  <variableDef name="CYr" varID="cyr" units="per rad" sign="+right/+right">
    <description>
      Damping derivative: lateral force increment per radian/sec of yaw rate
    </description>
  </variableDef>

  <variableDef name="CYp" varID="cyp" units="per rad" sign="+right/+right">
    <description>
      Damping derivative: lateral force increment per radian/sec of roll rate
    </description>
  </variableDef>

  <variableDef name="CZq" varID="czq" units="per rad" sign="+down/+up">
    <description>
      Damping derivative: vertical force increment per radian/sec of pitch rate
    </description>
  </variableDef>

  <variableDef name="Clr" varID="clr" units="per rad" sign="+right/+right">
    <description>
      Damping derivative: rolling moment increment per radian/sec of yaw rate
    </description>
  </variableDef>

  <variableDef name="Clp" varID="clp" units="per rad" sign="+right/+right">
    <description>
      Damping derivative: rolling moment increment per radian/sec of roll rate
    </description>
  </variableDef>

  <variableDef name="Cmq" varID="cmq" units="per rad" sign="+up/+up">
    <description>
      Damping derivative: pitching moment increment per radian/sec of pitch rate
    </description>
  </variableDef>

  <variableDef name="Cnr" varID="cnr" units="per rad" sign="+right/+right">
    <description>
      Damping derivative: yawing moment increment per radian/sec of yaw rate
    </description>
  </variableDef>

  <variableDef name="Cnp" varID="cnp" units="per rad" sign="+right/+right">
    <description>
      Damping derivative: yawing moment increment per radian/sec of roll rate
    </description>
  </variableDef>

  <variableDef name="ClDA" varID="dclda" units="per deg" sign="+right/+left">
    <description>
      Control power derivative: rolling moment per deg of aileron deflection
    </description>
  </variableDef>

  <variableDef name="ClDR" varID="dcldr" units="per deg" sign="+right/+left">
    <description>
      Control power derivative: rolling moment per deg of rudder deflection
    </description>
  </variableDef>

  <variableDef name="CnDA" varID="dcnda" units="per deg" sign="+right/+left">
    <description>
      Control power derivative: yawing moment per deg of aileron deflection
    </description>
  </variableDef>

  <variableDef name="CnDR" varID="dcndr" units="per deg" sign="+right/+left">
    <description>
      Control power derivative: yawing moment per deg of rudder deflection
    </description>
  </variableDef>

      <!-- ================== -->
      <!--  Output variables  -->
      <!-- ================== -->

  <variableDef name="CX" varID="cx" units="" sign="+FWD">
    <description>
	Total coefficient of force along the body X-axis.
    </description>
    <calculation>
      <math>
	<apply>
	  <plus/>
	  <ci>cxt</ci>
	  <apply>
	    <times/>
	    <ci>cq2v</ci>
	    <ci>cxq</ci>
	  </apply>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="CY" varID="cy" units="" sign="+RIGHT">
    <description>
	Total coefficient of force along the body Y-axis
    </description>
    <calculation>
      <math>
	<apply>
	  <plus/>
	  <ci>cy0</ci>
	  <apply>
	    <times/>
	    <ci>b2v</ci>
	    <apply>
	      <plus/>
	      <apply>
		<times/>
		<ci>cyp</ci>
		<ci>p</ci>
	      </apply>
	      <apply>
		<times/>
		<ci>cyr</ci>
		<ci>r</ci>
	      </apply>
	    </apply>
	  </apply>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="CZ" varID="cz" units="" sign="+DOWN">
    <description>
	Total coefficient of force along the body Z-axis.
    </description>
    <calculation>
      <math>
	<apply>
	  <plus/>
	  <ci>cz1</ci>
	  <apply>
	    <times/>
	    <ci>cq2v</ci>
	    <ci>czq</ci>
	  </apply>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="CLL" varID="cl" units="" sign="+RWD">
    <description>
	Total coefficient of moment around the body X-axis (rolling moment)
    </description>
    <calculation>
      <math>
	<apply>
	  <plus/>
	  <ci>cl1</ci>
	  <apply>
	    <times/>
	    <ci>b2v</ci>
	    <apply>
	      <plus/>
	      <apply>
		<times/>
		<ci>clp</ci>
		<ci>p</ci>
	      </apply>
	      <apply>
		<times/>
		<ci>clr</ci>
		<ci>r</ci>
	      </apply>
	    </apply>
	  </apply>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="CLM" varID="cm" units="" sign="+ANU">
    <description>
	Total coefficient of moment around the body Y-axis (pitching moment)
    </description>
    <calculation>
      <math>
	<apply>
	  <plus/>
	  <ci>cmt</ci>
	  <apply>
	    <times/>
	    <ci>cq2v</ci>
	    <ci>cmq</ci>
	  </apply>
	  <apply>
	    <times/>
	    <ci>cz</ci>
	    <apply>
	      <plus/>
	      <ci>xcgr</ci>
	      <apply>
		<minus/>
		<ci>xcg</ci>
	      </apply>
	    </apply>
	  </apply>
	</apply>
      </math>
    </calculation>
  </variableDef>

  <variableDef name="CLN" varID="cn" units="" sign="+ANR">
    <description>
	Total coefficient of moment around the body Z-axis (yawing moment)
    </description>
    <calculation>
      <math>
	<apply>   <!-- start of cn + b2v*() - cy*()*cbar/bspan -->
	  <plus/>
	  <ci>cn1</ci>
	  <apply>	<!-- start of b2v*(d(9)*p + d(8)*r) -->
	    <times/>
	    <ci>b2v</ci>
	    <apply>	<!-- start of (d(9)*p + d(8)*r) -->
	      <plus/>
	      <apply>	<!-- start of d(9)*p -->
		<times/>
		<ci>cnp</ci>	<!-- d(9) is cnp -->
		<ci>p</ci>
	      </apply>	<!-- end of d(9)*p -->
	      <apply>	<!-- start of d(8)*r -->
		<times/>
		<ci>cnr</ci>	<!-- d(8) is cnr -->
		<ci>r</ci>
	      </apply> <!-- end of d(8)*r -->
	    </apply>  <!-- end of (d(9)*p + d(8)*r) -->
	  </apply> <!-- end of b2v*(d(9)*p + d(8)*r) -->
	  <apply>	<!-- start of -cy*(xcgr-xcg)*cbar/bspan -->
	    <minus/>
	    <apply>	<!-- start of cy*(xcgr-xcg)*cbar/bspan -->
	      <quotient/>
	      <apply>	<!-- start of cy*(xcgr-xcg)*cbar -->
		<times/>
		<ci>cy</ci>
		<apply>	<!-- start of xcgr-xcg -->
		  <plus/>
		  <ci>xcgr</ci>
		  <apply>	<!-- start of -xcg -->
		    <minus/>
		    <ci>xcg</ci>
		  </apply>	<!-- end of -xcg -->
		</apply>	<!-- end of xcgr-xcg -->
		<ci>cbar</ci>
	      </apply>	<!-- end of cy*(xcgr-xcg)*cbar -->
	      <ci>bspan</ci>
	    </apply>	<!-- end of cy*(xcgr-xcg)*cbar/bspan -->
	  </apply>	<!-- end of -cy*(xcgr-xcg)*cbar/bspan -->
	</apply>   <!-- end of cn + b2v*() - cy*()*cbar/bspan -->
      </math>
    </calculation>
  </variableDef>

